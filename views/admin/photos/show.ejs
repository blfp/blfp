<main class='container'>
  <% include ../subnav %>
  <h1>Photo</h1>
  <hr>
  <div class='text-center'>
    <img src='<%= photo.imagePath('large') %>'>
  </div>
  <hr>
  <form method='post' action='/admin/photos/<%= photo.id %>/delete'>
    <button type='submit' class='btn btn-danger' data-confirm='Are you sure?'>
      Delete
    </button>
  </form>
</main>
