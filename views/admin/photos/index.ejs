<main class='container'>
  <% include ../subnav %>
  <h1>
    Photos
  </h1>
  <form class='form-inline' method='post' action='/admin/photos' enctype='multipart/form-data'>
    <input type='hidden' name='return_to' value='/admin/photos'>
    <input type='file' name='image' class='form-control'>
    <button type='submit' class='btn btn-default'>Upload</button>
  </form>
  <hr>
  <% photos.forEach(function (photo) { %>
    <a href='/admin/photos/<%= photo.id %>'>
      <img src='<%= photo.imagePath('medium') %>'>
    </a>
  <% }) %>
</main>
